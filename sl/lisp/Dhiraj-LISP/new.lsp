(defun main()	(princ "Enter size:")	(setq n(read))	(setq a(make-array n))	(princ "Enter array:")	(dotimes (i n)		(setf (aref a i) (read))	)	(dotimes (i n)		(dotimes (j (- n 1))			(if (> (aref a j) (aref a (+ j 1)))				(progn					(setf t (aref a j))					(setf (aref a j) (aref a (+ j 1)))					(setf (aref a (+ j 1)) t)				)			)		)	)	(print a)	(princ "Enter the number:")	(setq x(read))	(setf lb 0)	(setf ub (- n 1))		(do ((temp 0)) ((> lb ub))		(setf mid (truncate(/ (+ lb ub) 2)))		(if (= (aref a mid) x)			(progn							(princ "Element found at ")				(print mid)				(dos "pause")				(dos exit)				)			(progn				(if(> (aref a mid) x)				(progn					(setf ub (- mid 1))				)				)				(if(< (aref a mid) x)				(progn					(setf lb (+ mid 1))				)				)			)		)	)				)(main)